{
  "name": "m2-middleware",
  "version": "1.0.0",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "NODE_ENV=development nodemon src/server.js",
    "migrate": "node -e \"require('./src/db/db').migrate()\"",
    "migrate-mappings": "node scripts/migrate-to-file-mapping.js",
    "migrate-mappings:backup": "node scripts/migrate-to-file-mapping.js --backup",
    "migrate-mappings:full": "node scripts/migrate-to-file-mapping.js --backup --validate"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "better-sqlite3": "^11.7.0",
    "cron": "^2.2.0",
    "dotenv": "^16.0.0",
    "express": "^4.18.2",
    "winston": "^3.8.2",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "fast-check": "^4.3.0",
    "nodemon": "^2.0.22"
  }
}
